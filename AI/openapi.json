        "required": ["name", "numOfRequest"],
        "title": "PositionRequest"
      },
      "SkillRequest": {
        "properties": {
          "description": { "type": "string", "title": "Description" },
          "positions": {
            "items": { "$ref": "#/components/schemas/PositionRequest" },
